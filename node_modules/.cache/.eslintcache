atch = useDispatch();\r\n    const {dog_details, loading} = useSelector(state=>state);\r\n    const {id} = useParams();\r\n\r\n\r\n    useEffect(()=>{\r\n        dispatch(getDog(id));\r\n        return dispatch(cleanDogDetails())\r\n    },[id])\r\n\r\n console.log(dog_details);\r\n    return(\r\n        <div>\r\n        <h1 className={style.h1}> Exploring Breed </h1>\r\n       { loading ? <Loading/> :\r\n         <div className={style.card}>\r\n         <Link to='/home'><button className={style.button}> Go back </button></Link>\r\n         <p className={style.breedName}>{dog_details.name}</p>\r\n              <img  className={style.image} src= {dog_details.image} alt={dog_details.name}></img>\r\n              <div className={style.p}>\r\n                 <p>Id: {dog_details.id}</p>             \r\n                 <p>Height: {dog_details.height} cm.</p>\r\n                 <p>Weight: {dog_details.weight}kgs. </p>\r\n                 <p>Life Span: {dog_details.life_span}</p>\r\n                 <p>Temperaments: {dog_details.temperaments}</p>\r\n              </div>\r\n              \r\n         </div>\r\n       }\r\n              \r\n        </div>\r\n    )\r\n}\r\n\r\nexport default Detail;","C:\\Users\\Usuario\\Downloads\\CLIENT PI DOGS\\client\\src\\views\\LandingPage\\LandingPage.jsx",[],[],"C:\\Users\\Usuario\\Downloads\\CLIENT PI DOGS\\client\\src\\views\\Home\\Home.jsx",[],[],"C:\\Users\\Usuario\\Downloads\\CLIENT PI DOGS\\client\\src\\views\\About\\About.jsx",[],[],"C:\\Users\\Usuario\\Downloads\\CLIENT PI DOGS\\client\\src\\views\\Form\\Form.jsx",["113","114","115","116","117","118","119","120","121","122","123","124","125"],[],"import { useState , useEffect} from \"react\";\r\nimport style from './Form.module.css';\r\nimport { createDog, getTemperaments } from \"../../redux/actions\";\r\nimport { useDispatch, useSelector } from \"react-redux\";\r\n\r\nconst Form = () => {\r\n\r\n    const dispatch = useDispatch();\r\n    const {dogs_backup, temperaments} = 